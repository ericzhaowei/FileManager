<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ItemCardView">
        <attr name="mainImage" format="reference"/>
        <attr name="mainTitle" format="reference" />
    </declare-styleable>

    <declare-styleable name="FloatImage">
        <attr name="radius" format="dimension" />
        <attr name="image_width" format="dimension" />
        <attr name="image_height" format="dimension" />
        <attr name="text" format="reference" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="textWidth" format="dimension" />
        <attr name="startImage" format="reference"/>
        <attr name="stopImage" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SmbProgressbar">
        <attr name="reach_height" format="dimension" />
        <attr name="unreach_height" format="dimension" />
        <attr name="reach_color" format="color" />
        <attr name="unreach_color" format="color" />
        <attr name="text_color" format="color" />
        <attr name="text_size" format="dimension" />
    </declare-styleable>

</resources>